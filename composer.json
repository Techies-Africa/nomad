{
  "name": "techies-africa/nomad",
  "description": "Official laravel client for Nomad",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Joel Omojefe",
      "email": "joelomojefe@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "orchestra/testbench": "^8.23",
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "TechiesAfrica\\Nomad\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TechiesAfrica\\Nomad\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TechiesAfrica\\Nomad\\NomadServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-f": "vendor/bin/phpunit --filter"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
